                                        <div class="heading_s1">
                                            <h3 class="mb-30 text-dark text-center">Create an Account</h3>

                                            <% if (typeof successMessage !== 'undefined') { %>
                                                <p style="color: green; font-size: 25px;"><%= successMessage %></p>
                                            <% } else if (typeof errorMessage !== 'undefined') { %>
                                                <p style="color: red; font-size: 25px; border: 1px solid #FF0000; padding: 10px; border-radius: 5px; margin-bottom: 10px;"><%= errorMessage %></p>
                                            <% } %>

                                        </div>
                                        <form method="post" id="registrationForm">
                                            <div class="form-group">
                                                <input type="text" onblur="validateFirstName()"  name="firstname" id="firstname" placeholder="Enter Firstname"  data-bs-toggle="tooltip" data-bs-placement="top" title="firstname should only contain letters and cannot be empty"  data-bs-trigger="hover" data-bs-delay="500"  required>  
                                                <span class="error-message" id="firstNameErrorMessage"></span>
                                            </div>
                                            <div class="form-group">
                                                <input type="text"  name="lastname" id="lastname" placeholder="Enter Lastname" data-bs-toggle="tooltip" data-bs-placement="top" title="Lastname should only contain letters and cannot be empty"  data-bs-trigger="hover" data-bs-delay="500" required>
                                                <span class="error-message" id="lastnameErrorMessage"></span>
                                            </div>
                                            
                                            <div class="form-group">
                                                <input type="text"  name="username" id="username" placeholder="Enter Username" data-bs-toggle="tooltip" data-bs-placement="top" title="Username must be unique"  data-bs-trigger="hover" data-bs-delay="500" required>
                                                <span class="error-message" id="usernameErrorMessage"></span>
                                            </div>
                                            <div class="form-group">
                                                <input type="text"  name="email" id="emailId" placeholder="Enter Email-Id" data-bs-toggle="tooltip" data-bs-placement="top" title="Enter Email Id"  data-bs-trigger="hover" data-bs-delay="500" required>
                                                <span class="error-message" id="emailErrorMessage"></span>
                                            </div>
                                            <div class="form-group">
                                                <input type="text"  name="mobile" id="mobile" placeholder="Enter Phone no." data-bs-toggle="tooltip" data-bs-placement="top" title="Phone number shoul contain 10 digits and valid number"  data-bs-trigger="hover" data-bs-delay="500" required>
                                                <span class="error-message" id="phoneErrorMessage"></span>
                                            </div>
                                            <div class="form-group">
                                                <div class="password-container">
                                                    <input type="password" name="password" id="password" placeholder="Enter Password"  data-bs-toggle="tooltip" data-bs-placement="top" title="Password must contain at least one uppercase letter, one digit, and one special character"  data-bs-trigger="hover" data-bs-delay="500" required>
                                                    <span id="togglePassword" class="material-symbols-outlined eye-icon"  onclick="togglePasswordVisibility()">
                                                        visibility
                                                    </span>
                                                   
                                                </div>
                                                <span class="error-message" id="passwordErrorMessage"></span>
                                            </div>
                                            <div class="form-group">
                                                <input  type="password" name="confirm_password" placeholder="Confirm password" id="confirm_password" required>
                                                <span class="error-message" id="confirmPasswordErrorMessage"></span>
                                            </div>                                            
                                            <div class="login_footer form-group">
                                                <div class="chek-form">
                                                    <div class="custome-checkbox">
                                                        <input class="form-check-input" type="checkbox" name="checkbox" id="exampleCheckbox12" value="agree">
                                                        <label class="form-check-label" for="exampleCheckbox12"><span>I agree to terms &amp; Policy.</span></label>
                                                        <span class="error-message" id="checkboxErrorMessage"></span>
                                                    </div>
                                                </div>
                                                <a href="page-privacy-policy.html"><i class="fi-rs-book-alt mr-5 text-muted"></i>Learn more</a>
                                                
                                            </div>
                                            <div class="form-group">
                                                <button type="submit" class="btn btn-fill-out btn-block hover-up" id="submit">Submit</button>
                                            </div>
                                        </form>
                                        <div class="divider-text-center mt-15 mb-15">
                                            <span> or</span>
                                        </div>
                                        <!-- <ul class="btn-login list_none text-center mb-15">
