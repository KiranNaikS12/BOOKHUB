        <section class="content-main mt-80 mb-80" >
            <div class="card mx-auto card-login">
                <div class="card-body w-100" >
                    <h4 class="card-title mb-4 text-center fs-1" style="font-family: 'Times New Roman', Times, serif;">Register User</h4>
                    <% if(typeof message !== 'undefined'){ %>
                        <h4 style="color: red; font-family: 'Times New Roman', Times, serif;"><%=message %></h4>
                    <% } %>
                    <form accept="" method="post" id="registrationForm">
                        <div class="mb-3">
                            <label class="form-label">Firstname</label>
                            <input class="form-control" placeholder="Firstname" type="text" name="firstname" id="firstname">
                            <small class="error-message" id="firstNameErrorMessage"></small>
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Lastname</label>
                            <input class="form-control" placeholder="Lastname" type="text" name="lastname" id="lastname">
                            <small class="error-message" id="lastnameErrorMessage"></small>
                          </div>
                        <div class="mb-3">
                            <label class="form-label">Username</label>
                            <input class="form-control" placeholder="Username" type="text" name="username" id="username">
                            <small class="error-message" id="usernameErrorMessage"></small>
                          </div>
                        <div class="mb-3">
                            <label class="form-label">Email</label>
                            <input class="form-control" placeholder="Email" type="text" name="email" id="emailId" >
                            <small class="error-message" id="emailErrorMessage"></small>
                          </div> 
                        <div class="mb-3">
                            <label class="form-label">Phone</label>
                            <input class="form-control" placeholder="Phone no." type="text" name="mobile" id="mobile">
                            <small class="error-message" id="phoneErrorMessage"></small>
                          </div> 
                        <div class="mb-4">
                            <button type="submit" class="btn btn-primary w-100">Register User</button>
                        </div> 
                    </form>
                    
            </div>
        </section>
